generator client {
  provider = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder", "typedSql"]
  output = "../src/adapters/postgres/queries"

}

datasource db {
  provider = "postgresql"
  url      = env("ADAPTER_PRISMA_POSTGRES_URL")
}
